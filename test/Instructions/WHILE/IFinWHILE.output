define i32 @main() {
  entry:
    %a = alloca i32
    %0 = alloca i32
    store i32 10, i32* %0
    %1 = load i32, i32* %0
    store i32 %1, i32* %a
    br label %while1
  while1:
    %2 = load i32, i32* %a
    %3 = alloca i32
    store i32 0, i32* %3
    %4 = load i32, i32* %3
    %5 = icmp sgt i32 %2, %4
